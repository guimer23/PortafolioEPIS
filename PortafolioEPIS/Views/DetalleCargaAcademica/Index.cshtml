                                                <th>#</th>
                                                <th>Código</th>
                                                <th>Asignatura</th>
                                                <th>Tipo</th>
                                                <th>Docente</th>                                                
                                                <th>Seccion</th>
                                                <th class="text-center">Estado</th>
                                                <th class="text-right">Accción</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{ int num = 0;}
                                            @foreach (var p in Model)
                                            {
                                                if (p.Tbl_DetallePlanEstudio.Ciclo_DetallePlanEstudio == "I") {
                                                num = num + 1;
                                            <tr>
                                                <td>@num</td>
                                                <td><a href="~/DetalleCargaAcademica/Ver/@p.Tbl_DetallePlanEstudio.CodigoCurso_DetallePlanEstudio">@p.Tbl_DetallePlanEstudio.CodigoCurso_DetallePlanEstudio</a></td>
                                                <td>@p.Tbl_DetallePlanEstudio.Asignatura_DetallePlanEstudio</td>
                                                <td>@p.Tbl_DetallePlanEstudio.TipoCurso_DetallePlanEstudio</td>
                                                <td>@p.Tbl_Docente.Nombres_Docente, @p.Tbl_Docente.Apellidos_Docente</td>
                                                <td>@p.Tbl_Seccion.Nombre_Seccion</td>

                                                @if (p.Estado_DetalleCargaAcademica == true)
                                                {
                                                    <td><span class="custom-badge status-green">Activo</span></td>
                                                }
                                                else
                                                {
                                                    <td><span class="custom-badge status-red">Inactivo</span></td>
                                                }
